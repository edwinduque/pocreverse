{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "/": {
        "ClusterId": "blazor",
        "Match": {
          "Path": "/"
        }
      },
      "/about": {
        "ClusterId": "blazor",
        "Match": {
          "Path": "/about"
        }
      },
      "*": {
        "ClusterId": "webforms",
        "Match": {
          "Path": "/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "webforms": {
        "Destinations": {
          "webforms": {
            "Address": "https://localhost:44323/"
          }
        }
      },
      "blazor": {
        "Destinations": {
          "blazor": {
            "Address": "https://localhost:7005/"
          }
        }
      }
    }
  }
}
